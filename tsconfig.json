{
	"compilerOptions": {
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"jsx": "preserve",
		"lib": ["ES2017", "DOM"],
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "ES2017"
	}
}
