			"source": "src/front-end/index.html"
		},
		"backend": {
			"context": "node",
			"distDir": "dist/back-end",
			"source": "src/back-end/index.ts"
		}
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-organize-imports"
		],
		"vueIndentScriptAndStyle": false
	},
	"scripts": {
		"start": "npx parcel build && node dist/back-end/index.js"
	}
