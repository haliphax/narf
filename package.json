	},
	"engines": {
		"node": "^16 || ^18"
	},
	"targets": {
		"frontend": {
			"context": "browser",
			"distDir": "dist/front-end",
			"engines": {
				"browsers": "> 0.25%"
			},
			"source": "src/front-end/index.html"
		},
		"backend": {
			"context": "node",
			"distDir": "dist/back-end",
			"source": "src/back-end/index.ts"
		}
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-organize-imports"
		],
		"vueIndentScriptAndStyle": false
	},
	"scripts": {
		"build": "npx parcel build && npx ts-node src/tasks/clean.ts",
		"start": "node dist/back-end/index.js"
	}
